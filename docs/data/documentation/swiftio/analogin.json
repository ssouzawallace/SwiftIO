nt the analog input value every second.","while true {","    var value = pin.readVoltage()","    print(\"The analog input volatge is \\(value)\")","    sleep(ms: 1000)","}"]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/swiftio\/analogin"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/SwiftIO\/documentation\/SwiftIO\/AnalogIn","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"The AnalogIn class is used to read the external voltage applied to an analog"},{"type":"text","text":" "},{"type":"text","text":"input pin."}],"kind":"symbol","metadata":{"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"AnalogIn"}],"title":"AnalogIn","roleHeading":"Class","role":"symbol","symbolKind":"class","externalID":"s:7SwiftIO8AnalogInC","modules":[{"name":"SwiftIO"}],"navigatorTitle":[{"kind":"identifier","text":"AnalogIn"}]},"hierarchy":{"paths":[["doc:\/\/SwiftIO\/documentation\/SwiftIO"]]},"topicSections":[{"title":"Initializer","identifiers":["doc:\/\/SwiftIO\/documentation\/SwiftIO\/AnalogIn\/init(_:)"]},{"title":"Reading input","identifiers":["doc:\/\/SwiftIO\/documentation\/SwiftIO\/AnalogIn\/readPercent()","doc:\/\/SwiftIO\/documentation\/SwiftIO\/AnalogIn\/readRawValue()","doc:\/\/SwiftIO\/documentation\/SwiftIO\/AnalogIn\/readVoltage()"]},{"title":"Instance Properties","identifiers":["doc:\/\/SwiftIO\/documentation\/SwiftIO\/AnalogIn\/maxRawValue","doc:\/\/SwiftIO\/documentation\/SwiftIO\/AnalogIn\/obj","doc:\/\/SwiftIO\/documentation\/SwiftIO\/AnalogIn\/refVoltage"]}],"references":{"doc://SwiftIO/documentation/SwiftIO/AnalogIn/readRawValue()":{"role":"symbol","title":"readRawValue()","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"readRawValue"},{"kind":"text","text":"() -> "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"}],"abstract":[{"type":"text","te